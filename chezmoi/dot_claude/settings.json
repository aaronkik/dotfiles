{
  "env": {
    "DISABLE_TELEMETRY": 1
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "defaultMode": "acceptEdits",
    "allow": [
      "Bash(find:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git status:*)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git push:*)",
      "Bash(git pull:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git switch:*)",
      "Bash(git merge:*)",
      "Bash(git reset:*)",
      "Bash(git rebase:*)",
      "Bash(git stash:*)",
      "Bash(gh pr:*)",
      "Bash(gh issue:*)",
      "Bash(gh workflow:*)",
      "Bash(gh release:*)",
      "Bash(gh status:*)",
      "Bash(gh browse:*)",
      "Bash(ls:*)",
      "Bash(chezmoi add:*)",
      "Bash(chezmoi apply:*)",
      "Bash(chezmoi diff:*)",
      "Bash(chezmoi edit:*)",
      "Bash(chezmoi status:*)",
      "Bash(chezmoi update:*)",
      "Edit",
      "Glob",
      "Grep",
      "LS",
      "MultiEdit",
      "NotebookEdit",
      "NotebookRead",
      "Read",
      "Task",
      "TodoWrite",
      "WebFetch",
      "WebSearch",
      "Write"
    ],
    "deny": []
  }
}
